{
  "name": "nerd-junior",
  "version": "1.0.0",
  "description": "This is a utility bot that handles specific functions in Suggestions-related Discord servers.",
  "main": "dist/lib/index.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "nodemon src/index.ts",
    "start": "yarn run build && node dist/lib/index.js",
    "build": "rimraf dist && tsc",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint --fix ./src --ext .ts",
    "application-commands:register": "node dist/lib/register-application-commands.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suggestionsbot/nerd-junior.git"
  },
  "author": "Anthony Collier <hello@acollier.dev>",
  "license": "MIT",
  "engines": {
    "node": ">=14.x.x",
    "npm": "please-use-yarn",
    "yarn": ">=1.17.3"
  },
  "bugs": {
    "url": "https://github.com/suggestionsbot/nerd-junior/issues"
  },
  "homepage": "https://github.com/suggestionsbot/nerd-junior#readme",
  "devDependencies": {
    "@types/node": "14.14.9",
    "@types/ws": "7.4.7",
    "@typescript-eslint/eslint-plugin": "4.8.2",
    "@typescript-eslint/parser": "4.8.2",
    "eslint": "7.32.0",
    "rimraf": "3.0.2",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "@discordjs/builders": "^0.5.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "bufferutil": "^4.0.2",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.0.1",
    "dotenv": "^10.0.0",
    "erlpack": "discord/erlpack",
    "utf-8-validate": "^5.0.5",
    "zlib-sync": "^0.1.7"
  }
